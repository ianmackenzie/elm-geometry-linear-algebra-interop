{
    "version": "1.0.0",
    "summary": "Interop support for ianmackenzie/elm-geometry and elm-community/linear-algebra",
    "repository": "https://github.com/ianmackenzie/elm-geometry-linear-algebra-interop.git",
    "license": "MPL-2.0",
    "source-directories": [
        "src"
    ],
    "exposed-modules": [
        "Geometry.Interop.LinearAlgebra.Direction2d",
        "Geometry.Interop.LinearAlgebra.Direction3d",
        "Geometry.Interop.LinearAlgebra.Frame3d",
        "Geometry.Interop.LinearAlgebra.Point2d",
        "Geometry.Interop.LinearAlgebra.Point3d",
        "Geometry.Interop.LinearAlgebra.Vector2d",
        "Geometry.Interop.LinearAlgebra.Vector3d"
    ],
    "dependencies": {
        "elm-community/linear-algebra": "3.1.2 <= v < 4.0.0",
        "elm-lang/core": "5.1.1 <= v < 6.0.0",
        "ianmackenzie/elm-geometry": "1.0.0 <= v < 2.0.0"
    },
    "elm-version": "0.18.0 <= v < 0.19.0"
}
